{
    "name": "@microsoft/vscode-docker-registries",
    "author": "Microsoft Corporation",
    "version": "0.3.0",
    "description": "Extensibility model for contributing registry providers to the Container Tools extension for Visual Studio Code",
    "license": "See LICENSE in the project root for license information.",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-docker-extensibility"
    },
    "bugs": {
        "url": "https://github.com/microsoft/vscode-docker-extensibility/issues"
    },
    "homepage": "https://github.com/microsoft/vscode-docker-extensibility/blob/main/packages/vscode-docker-registries/README.md",
    "keywords": [
        "vscode",
        "docker"
    ],
    "module": "dist/esm/index.js",
    "main": "dist/cjs/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc --outDir dist/esm",
        "build:cjs": "tsc --outDir dist/cjs --module nodenext --moduleResolution nodenext",
        "lint": "eslint --max-warnings 0 . --ext ts",
        "test": "mocha",
        "unittest": "mocha --grep /unit/i",
        "package": "npm pack"
    },
    "engines": {
        "vscode": "^1.105.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/chai-as-promised": "^7.1.3",
        "@types/mocha": "^9.1.0",
        "@types/node": "22.x",
        "@types/vscode": "1.104.0",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "chai": "^4.3.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.2.0",
        "mocha": "^10.0.0",
        "tsx": "^4.20.6",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "dayjs": "^1.11.7"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "node-option": [
            "import=tsx"
        ],
        "spec": [
            "src/test/**/*.test.ts"
        ],
        "watch-files": [
            "src/**/*.ts"
        ]
    }
}
