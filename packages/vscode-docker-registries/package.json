{
    "name": "@microsoft/vscode-docker-registries",
    "author": "Microsoft Corporation",
    "version": "0.4.0",
    "description": "Extensibility model for contributing registry providers to the Container Tools extension for Visual Studio Code",
    "license": "See LICENSE in the project root for license information.",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-docker-extensibility"
    },
    "bugs": {
        "url": "https://github.com/microsoft/vscode-docker-extensibility/issues"
    },
    "homepage": "https://github.com/microsoft/vscode-docker-extensibility/blob/main/packages/vscode-docker-registries/README.md",
    "keywords": [
        "vscode",
        "docker"
    ],
    "module": "dist/esm/index.js",
    "main": "dist/cjs/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc --outDir dist/esm",
        "build:cjs": "tsc --outDir dist/cjs --module nodenext --moduleResolution nodenext",
        "lint": "eslint --max-warnings 0 . --ext ts",
        "test": "mocha",
        "package": "npm pack"
    },
    "engines": {
        "vscode": "^1.105.0"
    },
    "dependencies": {
        "dayjs": "^1.11.7"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "node-option": [
            "import=tsx"
        ],
        "spec": [
            "src/test/**/*.test.ts"
        ],
        "watch-files": [
            "src/**/*.ts"
        ]
    }
}
