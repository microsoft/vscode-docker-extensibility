{
  "name": "vscode-docker-registries-extension",
  "displayName": "vscode-docker-registries-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Refresh Registries V2",
        "icon": "$(refresh)",
        "command": "dockerRegistries2.refreshRegistries"
      },
      {
        "title": "Connect Registry V2",
        "icon": "$(plug)",
        "command": "dockerRegistries2.connectRegistry"
      },
      {
        "title": "Disconnect Registry V2",
        "icon": "$(debug-disconnect)",
        "command": "dockerRegistries2.disconnectRegistry"
      }
    ],
    "menus": {
      "view/title": [
        {
          "when": "view == dockerRegistries2",
          "command": "dockerRegistries2.refreshRegistries",
          "group": "navigation@0"
        }
      ],
      "view/item/context": [
        {
          "when": "view == dockerRegistries2 && viewItem =~ /commonregistryroot/i",
          "command": "dockerRegistries2.disconnectRegistry",
          "group": "destructive@0"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "dockerRegistries2",
          "name": "Docker Registries (v2)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "tsc --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@microsoft/vscode-azext-azureauth": "^1.0.0"
  }
}
