{
    "name": "vscode-container-runtimes-extension",
    "version": "0.0.1",
    "publisher": "ms-azuretools",
    "displayName": "Alternate Container Runtime Support",
    "description": "Support for alternative container runtimes in the VS Code Docker extension",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "podman",
        "nerdctl",
        "rancher",
        "container"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-docker-extensibility"
    },
    "bugs": {
        "url": "https://github.com/microsoft/vscode-docker-extensibility/issues"
    },
    "homepage": "https://github.com/microsoft/vscode-docker-extensibility/blob/main/packages/vscode-container-runtimes-extension/README.md",
    "activationEvents": [
        "onCommand:vscode-docker.activateRuntimeProviderExtensions"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run build",
        "watch": "webpack --watch",
        "build": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint --max-warnings 0 . --ext ts"
    },
    "devDependencies": {
        "@types/node": "16.x",
        "@types/vscode": "1.63.1",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.4",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@microsoft/vscode-container-runtimes": "file:../../microsoft-vscode-container-runtimes-0.0.1-alpha.tgz",
        "vscode-nls": "^5.0.1"
    },
    "extensionDependencies": [
        "ms-azuretools.vscode-docker"
    ]
}
