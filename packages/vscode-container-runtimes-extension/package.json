{
    "name": "vscode-container-runtimes-extension",
    "version": "0.0.1",
    "publisher": "ms-azuretools",
    "displayName": "Alternate Container Runtime Support",
    "description": "Support for alternative container runtimes",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "podman",
        "nerdctl",
        "rancher",
        "container",
        "multi-root ready"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-docker-extensibility"
    },
    "bugs": {
        "url": "https://github.com/microsoft/vscode-docker-extensibility/issues"
    },
    "homepage": "https://github.com/microsoft/vscode-docker-extensibility/blob/main/packages/vscode-container-runtimes-extension/README.md",
    "activationEvents": [
        "onCommand:vscode-docker.activateRuntimeProviderExtensions"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint --max-warnings 0 . --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "16.x",
        "@types/vscode": "1.63.1",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.4",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@microsoft/vscode-container-runtimes": "^0.0.1-alpha"
    }
}
